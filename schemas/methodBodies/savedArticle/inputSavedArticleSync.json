{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "tagItems": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "article_id": {
            "type": "integer"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        },
        "required": [
          "article_id",
          "tags"
        ]
      }
    }
  },
  "required": [
    "saved_articles"
  ]
}
